{
  "name": "react-native-typescript-app",
  "version": "1.0.0",
  "description": "React Native application cu TypeScript È™i suport cross-platform",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "react-native clean",
    "reset-cache": "react-native start --reset-cache",
    
    "_comment": "=== DOCKER COMMANDS ===",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    
    "_comment2": "=== SALES PLATFORM API COMMANDS ===",
    "api": "node backend/server.js",
    "admin": "node backend/admin-server.js",
    "webhooks": "node backend/webhooks-server.js",
    "jobs": "node backend/jobs/processor.js",
    "email": "node backend/services/email-service.js",
    
    "_comment3": "=== DATABASE COMMANDS ===",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "db:create": "npx sequelize-cli db:create",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:studio": "npx prisma studio",
    
    "_comment4": "=== BUILD COMMANDS ===",
    "build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "react-native run-ios --configuration Release",
    "build:web": "expo build:web",
    "build:prod": "npm run build:web && npm run build:android",
    
    "_comment5": "=== PM2 PRODUCTION COMMANDS ===",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:reload": "pm2 reload ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    
    "_comment6": "=== DEPLOYMENT COMMANDS ===",
    "deploy:setup": "pm2 deploy ecosystem.config.js production setup",
    "deploy": "pm2 deploy ecosystem.config.js production",
    "deploy:revert": "pm2 deploy ecosystem.config.js production revert 1"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-vector-icons": "^10.0.0",
    "@react-native-async-storage/async-storage": "^1.19.3",
    
    "_comment": "=== E-COMMERCE & SALES PLATFORM DEPENDENCIES ===",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    
    "_comment2": "=== DATABASE & ORM ===",
    "pg": "^8.11.3",
    "sequelize": "^6.35.1",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "redis": "^4.6.11",
    
    "_comment3": "=== AUTHENTICATION & SECURITY ===",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-facebook": "^3.0.0",
    
    "_comment4": "=== PAYMENT PROCESSING ===",
    "stripe": "^14.9.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    
    "_comment5": "=== EMAIL & NOTIFICATIONS ===",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.3",
    "firebase-admin": "^11.11.1",
    
    "_comment6": "=== FILE UPLOAD & STORAGE ===",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1501.0",
    "cloudinary": "^1.41.0",
    
    "_comment7": "=== VALIDATION & UTILITIES ===",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    
    "_comment8": "=== MOBILE SPECIFIC ===",
    "react-native-camera": "^4.2.1",
    "react-native-image-picker": "^7.1.0",
    "react-native-permissions": "^4.1.1",
    "react-native-push-notification": "^8.1.1",
    "@react-native-community/netinfo": "^11.2.1",
    "react-native-device-info": "^10.11.0",
    "react-native-keychain": "^8.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.72.2",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "react-native",
    "typescript",
    "mobile",
    "ios",
    "android",
    "cross-platform"
  ],
  "author": "React Native Developer",
  "license": "MIT"
}